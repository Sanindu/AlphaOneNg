{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "AlphaOne": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/AlphaOne",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [

              "assets/js/constants.js",
              "assets/js/app.js",
             "assets/js/document.js",
             "assets/js/english.js",
             "assets/js/sinhala.js",
             "assets/js/tamil.js",
             "assets/js/keyboard.js",
              "assets/js/englishHouseNumber.js",
             "assets/js/englishNIC.js",
              "assets/js/englishProfession.js",
              "assets/js/englishTitle.js",
             "assets/js/form.js",

             "assets/js/numberpad.js",
              "assets/js/sihalaProfession.js",

            "assets/js/sinhalaEmail.js",
              "assets/js/sinhalaHouseNumber.js",
              "assets/js/sinhalaNIC.js",
             "assets/js/sinhalaNumberpad.js",
             "assets/js/sinhalaTitle.js",

              "assets/js/tamilEmail.js",
              "assets/js/tamilHouseNumber.js",
             "assets/js/tamilNIC.js",
              "assets/js/tamilNumberpad.js",
             "assets/js/tamilProfession.js",
             "assets/js/tamilTitle.js",
             "assets/js/voice.js"

















            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "AlphaOne:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "AlphaOne:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "AlphaOne:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "AlphaOne:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "AlphaOne:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "AlphaOne"
}
